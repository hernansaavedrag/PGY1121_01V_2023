Proceso calculos
	
	definir suma,num1,num2,resta,multi como entero;
	Definir div como real;
	
	Escribir "Ingrese un número: ";
	leer num1;
	
	Escribir "Ingrese otro número: ";
	leer num2;
	
	suma = num1 + num2;
	resta = num1 - num2; //asumir que num1 es mayor que num2
	multi = num1 * num2;
	div = num1/num2;
	
	Escribir num1," + ",num2," = ",suma ;
	Escribir num1," - ",num2," = ",resta ;
	Escribir num1," * ",num2," = ",multi ;
	Escribir num1," / ",num2," = ",div ;
	
FinProceso
